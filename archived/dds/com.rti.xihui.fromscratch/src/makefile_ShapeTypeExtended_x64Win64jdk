JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES =  ./shape/ShapeTypeExtended.java ./shape/ShapeTypeExtendedSeq.java ./shape/ShapeTypeExtendedTypeSupport.java ./shape/ShapeTypeExtendedTypeCode.java ./shape/ShapeTypeExtendedDataReader.java ./shape/ShapeTypeExtendedDataWriter.java ./shape/ShapeTypeExtendedSubscriber.java ./shape/ShapeTypeExtendedPublisher.java

CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/class/nddsjava.jar
 
%.class : %.java
	$(JAVAC_PATH)  -classpath ".;$(RTI_CLASSPATH)" $<
 
all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export Path := $(NDDSHOME)/lib/x64Win64jdk;$(Path)

ShapeTypeExtendedPublisher: ./shape/ShapeTypeExtendedPublisher.class
	$(JAVA_PATH) -classpath ".;$(RTI_CLASSPATH)"  shape.ShapeTypeExtendedPublisher $(ARGS)
 
ShapeTypeExtendedSubscriber: ./shape/ShapeTypeExtendedSubscriber.class
	$(JAVA_PATH) -classpath ".;$(RTI_CLASSPATH)"  shape.ShapeTypeExtendedSubscriber $(ARGS)
